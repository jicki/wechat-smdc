# 镜像仓库和版本
REGISTRY=jicki
VERSION=latest

# 时区和Java配置
TZ=Asia/Shanghai
JAVA_OPTS=-Xmx512m -Xms512m

# 端口配置
APP_PORT=8081
ADMIN_PORT=8080
MYSQL_PORT=3306
REDIS_PORT=6379

# MySQL配置
MYSQL_DATABASE=smdcmanage
MYSQL_ROOT_PASSWORD=123456
MYSQL_USER=root
MYSQL_PASSWORD=123456

# Redis配置
REDIS_PASSWORD=

# JWT配置
JWT_SECRET=smdcmanage_secret_key
JWT_EXPIRATION=604800

# 微信配置
WX_APPID=your-appid
WX_SECRET=your-secret
WX_MCHID=your-mchid
WX_KEY=your-key
WX_NOTIFY_URL=http://your-domain/api/pay/notify

# 上传目录
UPLOAD_DIR=./uploads